{
  "name": "twitter emoji",
  "version": "0.2.0",
  "manifest_version": 2,
  "description": "Simple inject of twitter emojis in Chrome",
  "homepage_url": "https://github.com/benjick/chrome-twemoji",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "css": [
        "src/inject/inject.css"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}